pipeline {
  agent any

  tools {
    maven 'M2_HOME'
    }
  
  stages {
    stage('CheckOut') {
      steps {
        echo 'Checkout the source code from GitHub'
        git 'https://github.com/Harshit-reddy-1998/star-agile-insurance-project.git'
            }
    }
    stage('Build and Package Application') {
      steps {
        echo 'Build and Package App'
        sh 'mvn clean package'
            }
    }          
}   
}
